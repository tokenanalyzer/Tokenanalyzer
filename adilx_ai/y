import requests

def fetch_token_data():
    solana_pair_address = "A8nPhp...k7VC"  # Official Trump Pair Address (Solana)
    url = f"https://api.dexscreener.com/latest/dex/pairs/solana/{solana_pair_address}"

    response = requests.get(url)
    if response.status_code == 200:
        data = response.json()
        print("ğŸ” API Status Code:", response.status_code)
        print("ğŸ“Š API Response:", data)

        pair = data.get("pairs", [])
        if pair:
            return pair[0]  # Return first pair data
        else:
            print("âŒ Error: No valid token data found.")
            return None
    else:
        print("âŒ API Request Failed! Status Code:", response.status_code)
        return None

# Run function and print data
print(fetch_token_data())
